                                  <div class="kt-widget__item d-flex align-items-center kt-margin-r-30">
                                    <span class="kt-widget__date kt-padding-0 kt-margin-r-10">
                                      开始
                                    </span>
                                    <div class="kt-widget__label">
                                      <span class="btn btn-label-brand btn-sm btn-bold btn-upper" ng-bind="course.courseTimeBegin"></span>
                                    </div>
                                  </div>
                                  <div class="kt-widget__item d-flex align-items-center kt-padding-l-0">
                                    <span class="kt-widget__date kt-padding-0 kt-margin-r-10 ">
                                      结束
                                    </span>
                                    <div class="kt-widget__label">
                                      <span class="btn btn-label-danger btn-sm btn-bold btn-upper" ng-bind="course.courseTimeEnd"></span>
                                    </div>
                                  </div>
                                </div>
                                <div class="kt-widget__container">
                                  <span class="kt-widget__subtitel">Progress</span>
                                  <div class="kt-widget__progress d-flex align-items-center flex-fill">
                                    <div class="progress" style="height: 5px;width: 100%;">
                                      <div class="progress-bar kt-bg-success"
                                           role="progressbar"
                                           ng-style="{'width':course.courseProcess}">
                                      </div>
                                    </div>
                                    <span class="kt-widget__stat" ng-bind="course.courseProcess">
																    </span>
                                  </div>
                                </div>
                              </div>
                              <div class="kt-widget__footer">
                                <div class="kt-widget__wrapper">
                                  <div class="kt-widget__section">
                                    <div class="kt-widget__blog">
                                      <i class="flaticon-list-3" style="color: #3d3d3e"></i>
                                      <a href="javascript:;" class="kt-widget__value kt-font-brand no-hand">
                                        覆盖知识点 \{{course.courseKnowledgeCount}}/\{{course.technologyKnowledgeCount}}
                                      </a>
                                    </div>
                                    <div class="kt-widget__blog">
                                      <i class="flaticon-list" style="color: #3d3d3e"></i>
                                      <a href="javascript:;" class="kt-widget__value kt-font-brand no-hand">
                                        覆盖练习题 \{{course.courseKnowledgeExercisesCount}}
                                      </a>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                            <!--end::Widget -->
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row" ng-if="model.isLogin && model.courseProcessing4UniversityList.length > 0 && model.courseProcessing4UniversityList.length < model.courseProcessing4UniversityTotalCount">
                      <div class="col-xl-12 text-center">
                        <button type="button" class="btn btn-secondary btn-elevate btn-pill" ng-click="onLoadMoreCourseSelfUniversity()" ><i class="fa fa-sync-alt"></i>加载更多</button>
                      </div>
                    </div>
                    <div class="row" ng-if="model.isLogin && model.courseProcessing4UniversityList.length === 0">
                      <div class="col-xl-12 text-center">
                        <div class="alert alert-secondary" role="alert">
                          <div class="alert-text">您所在的院校尚未开设该技术的相关课程</div>
                        </div>
                      </div>
                    </div>
                    <div class="row" ng-if="model.isLogin == false">
                      <div class="col-xl-12">
                        <div class="kt-portlet">
                          <div class="kt-portlet__body">
                            <div class="kt-widget kt-widget--user-profile-3 text-center">
                              您还没有登陆，<a class="kt-font-brand kt-link kt-font-bold" ng-href="/login?backUrl=/technology?technology=\{{model.technologyID}}">现在登陆</a>查看该技术本校开设的课程
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- end:: 正在进行中的本校课程 -->

                  <!-- begin:: 正在进行中的外校课程 -->
                  <div class="tab-pane" id="kt_portlet_base_demo_2_2_tab_content" role="tabpanel">
                    <div class="row" ng-if="model.isLogin && model.courseProcessing4OtherUniversityList.length > 0">
                      <div class="col-xl-4" ng-repeat="course in model.courseProcessing4OtherUniversityList">
                        <div class="kt-portlet kt-portlet--height-fluid">
                          <div class="kt-portlet__body kt-portlet__body--fit">
                            <!--begin::Widget -->
                            <div class="kt-widget kt-widget--project-1">
                              <div class="kt-widget__head d-flex">
                                <div class="kt-widget__label" style="margin-left: -15px">
                                  <div class="kt-widget__media kt-widget__media--m">
                                    <span class="kt-media kt-media--md kt-media--circle kt-hidden-">
                                      <img src="/media/users/teacher_default.png" ng-if="course.teacherPhotoUrl == ''" alt="image">
                                      <img ng-src="\{{course.teacherPhotoUrl}}" ng-if="course.teacherPhotoUrl != ''" alt="image">
                                    </span>
                                  </div>
                                  <div class="kt-widget__info kt-padding-0 kt-margin-l-15">
                                    <a href="#" class="kt-widget__title" ng-bind="course.courseName">
                                    </a>
                                    <span class="kt-widget__desc">
                                      \{{course.universityName}} \{{course.teacherName}}
                                    </span>
                                  </div>
                                </div>
                              </div>
                              <div class="kt-widget__body">
                                <p class="kt-widget__text kt-margin-t-0 kt-padding-t-5 course-decs" ng-bind="course.courseIntroduction"></p>
                                <div class="kt-widget__stats kt-margin-t-20">
                                  <div class="kt-widget__item d-flex align-items-center kt-margin-r-30">
                                    <span class="kt-widget__date kt-padding-0 kt-margin-r-10">
                                      开始
                                    </span>
                                    <div class="kt-widget__label">
                                      <span class="btn btn-label-brand btn-sm btn-bold btn-upper" ng-bind="course.courseTimeBegin"></span>
                                    </div>
                                  </div>
                                  <div class="kt-widget__item d-flex align-items-center kt-padding-l-0">
                                    <span class="kt-widget__date kt-padding-0 kt-margin-r-10 ">
