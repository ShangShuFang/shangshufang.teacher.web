<link href="/stylesheets/views/technology.css" rel="stylesheet" type="text/css"/>

<div ng-app="pageApp" ng-controller="pageCtrl">

  <!-- begin:: 技术简介 -->
  <div class="technology-header">
    <div class="kt-container kt-grid__item kt-grid__item--fluid">
      <div class="row">
        <div class="col-xl-12">
          <div class="technology-header-portlet">
            <div class="kt-portlet__body">
              <div class="kt-widget kt-widget--user-profile-3">
                <div class="kt-widget__top">
                  <div class="kt-font-boldest kt-font-light technology-name">
                    <input type="hidden" value="{{technologyID}}" id="hidden_technologyID">
                    <h3 ng-bind="model.technologyInfo.technologyName"></h3>
                    <span class="kt-widget__desc">
                      <i class="la" ng-class="{'la-star-o': model.technologyInfo.technologyStars < 1, 'la-star star-selected': model.technologyInfo.technologyStars >= 1}"></i>
                      <i class="la" ng-class="{'la-star-o': model.technologyInfo.technologyStars < 2, 'la-star star-selected': model.technologyInfo.technologyStars >= 2}"></i>
                      <i class="la" ng-class="{'la-star-o': model.technologyInfo.technologyStars < 3, 'la-star star-selected': model.technologyInfo.technologyStars >= 3}"></i>
                      <i class="la" ng-class="{'la-star-o': model.technologyInfo.technologyStars < 4, 'la-star star-selected': model.technologyInfo.technologyStars >= 4}"></i>
                      <i class="la" ng-class="{'la-star-o': model.technologyInfo.technologyStars < 5, 'la-star star-selected': model.technologyInfo.technologyStars >= 5}"></i>
                  </span>
                  </div>
                  <div class="kt-widget__content">
                    <div class="color-white">
                      <h5 >
                        <span ng-bind="model.technologyInfo.technologyName"></span><span>简介</span>
                      </h5>
                    </div>
                    <div class="kt-widget__info">
                      <div class="kt-widget__desc">
                        <p class="technology-decs color-white" ng-bind="model.technologyInfo.technologyMemo"></p>
                      </div>
                    </div>
                    <div class="kt-widget__action">
                      <button type="button" class="btn btn-brand btn-upper" ng-click="onCreateCourse()">创建课程</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end:: 技术简介 -->

  <div class="kt-subheader kt-grid__item">
    <div class="kt-container">
      <div class="kt-subheader__main">
        <h3 class="kt-subheader__title"></h3>
      </div>
    </div>
  </div>

  <!-- begin:: 知识点列表 -->
  <div class="kt-container kt-grid__item kt-grid__item--fluid">
    <div class="kt-portlet">
      <div class="kt-portlet__head">
        <div class="kt-portlet__head-label">
          <h3 class="kt-portlet__head-title">
            知识点列表
            <small class="count">
              <strong>包含<span ng-bind="model.knowledgeTotalCount"></span>个知识点</strong>
            </small>
          </h3>
        </div>
        <div class="kt-portlet__head-label" ng-if="model.knowledgeList.length < model.knowledgeTotalCount">
          <a class="kt-font-brand refresh kt-font-bold" title="加载更多知识点" ng-click="onLoadMoreKnowledge()">+更多</a>
        </div>

      </div>
      <div class="kt-portlet__body">
        <div class="row">
          <div class="col-xl-6">
            <div class="kt-list-timeline">
              <div class="kt-list-timeline__items">

                <div class="kt-list-timeline__item" ng-repeat="knowledge in model.knowledgeList1">
                  <span class="kt-list-timeline__badge" ng-class="{'kt-list-timeline__badge--success': knowledge.learningPhaseID % 2 == 1, 'kt-list-timeline__badge--danger': knowledge.learningPhaseID % 2 == 0}"></span>
                  <span class="kt-list-timeline__text">
                    <span class="kt-badge kt-badge--inline"
                          ng-class="{'kt-badge--success': knowledge.learningPhaseID % 2 == 1, 'kt-badge--danger': knowledge.learningPhaseID % 2 == 0}"
                          ng-bind="knowledge.learningPhaseName"></span>
                    <span ng-bind="knowledge.knowledgeName"></span>

                  </span>
                  <span class="kt-list-timeline__time using-company">使用企业<strong class="count" ng-bind="knowledge.usingCompanyCount"></strong>家</span>
                </div>

              </div>
            </div>
          </div>
          <div class="col-xl-6">
            <div class="kt-list-timeline">
              <div class="kt-list-timeline__items">
                <div class="kt-list-timeline__item" ng-repeat="knowledge in model.knowledgeList2">
                  <span class="kt-list-timeline__badge" ng-class="{'kt-list-timeline__badge--success': knowledge.learningPhaseID % 2 == 1, 'kt-list-timeline__badge--danger': knowledge.learningPhaseID % 2 == 0}"></span>
                  <span class="kt-list-timeline__text">
                    <span class="kt-badge kt-badge--inline"
                          ng-class="{'kt-badge--success': knowledge.learningPhaseID % 2 == 1, 'kt-badge--danger': knowledge.learningPhaseID % 2 == 0}"
                          ng-bind="knowledge.learningPhaseName"></span>
                    <span ng-bind="knowledge.knowledgeName"></span>
                  </span>
                  <span class="kt-list-timeline__time using-company">使用企业<strong class="count" ng-bind="knowledge.usingCompanyCount"></strong>家</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end:: 知识点列表 -->

  <div class="space-10"></div>

  <!-- begin:: 市场研发方向 -->
  <div class="kt-container kt-grid__item kt-grid__item--fluid">
    <div class="kt-portlet">
      <div class="kt-portlet__head">
        <div class="kt-portlet__head-label">
          <h3 class="kt-portlet__head-title">
            市场研发方向
            <small class="count">
              <strong>涉及<span ng-bind="model.directionTotalCount"></span>个研发方向</strong>
            </small>
          </h3>
        </div>
      </div>
      <div class="kt-portlet__body">
        <div class="row">
          <div class="col-xl-12">
            <div class="kt-list-timeline">
              <div class="kt-list-timeline__items">
                <div class="kt-list-timeline__item" ng-repeat="direction in model.directionList">
                  <span class="kt-list-timeline__badge kt-list-timeline__badge--warning"></span>
                  <span class="kt-list-timeline__text" ng-bind="direction.directionName"></span>
                  <span class="kt-list-timeline__time direction-stars">
                    市场热度
                    <span class="kt-widget__desc">
                      <i class="la" ng-class="{'la-star-o': direction.directionStars < 1, 'la-star star-selected': direction.directionStars >= 1}"></i>
                      <i class="la" ng-class="{'la-star-o': direction.directionStars < 2, 'la-star star-selected': direction.directionStars >= 2}"></i>
                      <i class="la" ng-class="{'la-star-o': direction.directionStars < 3, 'la-star star-selected': direction.directionStars >= 3}"></i>
                      <i class="la" ng-class="{'la-star-o': direction.directionStars < 4, 'la-star star-selected': direction.directionStars >= 4}"></i>
                      <i class="la" ng-class="{'la-star-o': direction.directionStars < 5, 'la-star star-selected': direction.directionStars >= 5}"></i>
                  </span>
                  </span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end:: 市场研发方向 -->

  <div class="space-10"></div>

  <!-- begin:: 校内外开设课程 -->
